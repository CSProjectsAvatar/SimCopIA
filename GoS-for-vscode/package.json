{
    "name": "gos",
    "displayName": "GoS",
    "description": "Syntax highlight for the GoS language.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gos",
            "aliases": ["gos", "gos"],
            "extensions": [".gos"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gos",
            "scopeName": "source.gos",
            "path": "./syntaxes/gos.tmLanguage.json"
        }]
    },
    "repository": "https://github.com/CSProjectsAvatar/SimCopIA"
}