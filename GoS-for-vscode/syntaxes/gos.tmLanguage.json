{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "gos",
	"patterns": [
		{ "include": "#comments" },
		{ "include": "#keywords" },
		{ "include": "#strings" },
		{ "include": "#numbers" },
		{ "include": "#ids" }
	],
	"repository": {
		"comments": {
			"patterns": [{
					"name": "comment.line.number-sign.gos",
					"begin": "#",
					"while": "(?=not)possible"
				}]
		},
		"keywords": {
			"patterns": [{
					"name": "keyword.control.gos",
					"match": "\\b(if|else_if|else|return|forever|break|for|in)\\b"
				}, {
					"name": "keyword.other.gos",
					"match": "\\b(fun|let|print|new|and|or)\\b"
				}, {
					"name": "keyword.operator.gos",
					"match": ">|<|\\=|==|\\+|\\->|\\-|\\*|\\/|%"
				}, {
					"name": "support.class.gos",
					"match": "\\b(simplew|distw)\\b"
				}]
		},
		"strings": {
			"name": "string.quoted.double.gos",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.gos",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"patterns": [{
				"name": "constant.numeric.gos",
				"match": "[0-9]+(\\.[0-9]+)?"
			}]
		},
		"ids": {
			"patterns": [{
					"comment": "Llamado a funci√≥n.",
					"name": "support.function.gos",
					"match": "\\b_?[a-zA-Z][_a-zA-Z0-9]*\\b(?=\\s*\\()"
				}, {
					"comment": "Variable",
					"name": "variable.other.gos",
					"match": "\\b_?[a-zA-Z][_a-zA-Z0-9]*\\b"
				}]
		}
	},
	"scopeName": "source.gos"
}